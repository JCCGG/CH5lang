import printf "msvcrt.dll"
fun printf a string
fun println str string{
    str printf
    "\n"printf
}
init{
    "第一次加载这个文件时运行" println
}
main{
    val a = 100 / 2 + 10
    if a == 50
        "a==50是对的" println
    else if a == 60
        "a==60是对的！" println
    else
        "都不是" println

    "运行结束！" println
}